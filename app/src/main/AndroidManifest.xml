        android:supportsRtl="true"
        android:theme="@android:style/Theme.Holo.NoActionBar.Fullscreen"
        tools:replace="android:allowBackup">
        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />
        <activity
            android:name=".OutBrainContentActivity"
            android:theme="@style/AppTheme"
            android:exported="false" />
        <activity
            android:name=".ClassicWithImageActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".gridlayout.GridWithImagesActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SmartFeed.SmartFeedActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SmartFeed.SmartFeedMidFeedActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".admob.InLineBannerAdActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".VideoWidget.VideoWidgetActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SFWebView.SFWebViewRecyclerViewActivity"
            android:exported="false"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".SFWebView.SFWebViewScrollViewActivity"
